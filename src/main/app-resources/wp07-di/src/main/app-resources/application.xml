<?xml version="1.0" encoding="us-ascii"?>
<application xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" id="ISD">
<jobTemplates>
    <jobTemplate id="ISD_00">
      <streamingExecutable>/application/run.sh</streamingExecutable>
      <defaultParameters>
        <parameter id="JobID" title="JobID" abstract="JobID" scope="runtime" maxOccurs="1">2010</parameter>
        <parameter id="aoi" title="AOI" abstract="Select the AOI you want to process" scope="runtime" maxOccurs="1">AOI1</parameter>
      </defaultParameters>
    </jobTemplate>
  </jobTemplates>
    <workflow id="ISD_00" title="Parameters: BISD" abstract="The Biophysical Indicator of Susceptibility to Drought">
    <workflowVersion>1.0</workflowVersion>
    <node id="node_cli_block_a">
      <job id="ISD_00"/>
      <sources>
        <source id="Year" title="Year" abstract="Select the year(s) you want to process" refid="string:list" scope="runtime"/>
      </sources>
      <parameters>
           <parameter id="aoi">
    <options>
        <option>AOI1</option>
        <option>AOI3</option>
        <option>AOI4</option>
    </options>
           </parameter>
      </parameters>
    </node>
  </workflow>
</application>
