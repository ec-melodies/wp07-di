#!/bin/sh
#-------------------------------------------------------------------------------------# 
# PURPOSE: ISD
#-------------------------------------------------------------------------------------# 
start=`date +%s`
#-------------------------------------------------------------------------------------# 
export DIR=/data/auxdata/ISD/ISD000/
export -p IDIR=/application/
echo $IDIR
export -p IXDIR=$IDIR/processing_block_a/bin/
#-------------------------------------------------------------------------------------# 
#-------------------------------------------------------------------------------------# 
function igcx(){
exec $IXDIR"isd_vx001.sh" 
}

igcx
#-------------------------------------------------------------------------------------# 
#-------------------------------------------------------------------------------------# 
end=`date +%s`
runtime=$((end-start))
echo "Runtime: $runtime seconds"
#-------------------------------------------------------------------------------------# 
exit 0 