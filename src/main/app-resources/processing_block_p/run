#!/bin/sh
#-------------------------------------------------------------------------------------# 
# PURPOSE: isd_cx
#-------------------------------------------------------------------------------------# 
start=`date +%s`
#-------------------------------------------------------------------------------------#
export -p IXDIR=/application/processing_block_p/bin/

#-------------------------------------------------------------------------------------# 

#-------------------------------------------------------------------------------------# 
function igcx(){
exec $IXDIR"isd_cx001.sh" &
exec $IXDIR"isd_dx001.sh"
}

igcx
#-------------------------------------------------------------------------------------# 
#-------------------------------------------------------------------------------------# 
end=`date +%s`
runtime=$((end-start))
echo "Runtime: $runtime seconds"
#-------------------------------------------------------------------------------------# 
exit 0 