#!/bin/sh
#-------------------------------------------------------------------------------------# 
# PURPOSE: isd_cx
#-------------------------------------------------------------------------------------# 
start=`date +%s`
#-------------------------------------------------------------------------------------#

export -p IDIR=/application/
echo $IDIR
export -p IXDIR=$IDIR/processing_block_p/bin/

#-------------------------------------------------------------------------------------# 

#-------------------------------------------------------------------------------------# 
function igcx(){
exec $IXDIR"isd_cx001.sh" $1 $2 &
wait
exec $IXDIR"isd_dx001.sh" $1 $2
}

igcx
#-------------------------------------------------------------------------------------# 
#-------------------------------------------------------------------------------------# 
end=`date +%s`
runtime=$((end-start))
echo "Runtime: $runtime seconds"
#-------------------------------------------------------------------------------------# 
exit 0 